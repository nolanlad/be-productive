{

    "description": "Redirects you from time wasting websites and encourages language learning instead.",
    "manifest_version": 2,
    "name": "be-productive",
    "version": "1.0",
    "homepage_url": "https://github.com/nolanlad/be-productive",
    "icons": {
      "48": "pen.svg"
    },
  
    "permissions": [
      "webRequest", "webRequestBlocking", "<all_urls>" 
    ],
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }
    }
}
